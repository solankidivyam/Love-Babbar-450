#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int n,k;
    cin>>n>>k;
    vector <int > v;
    for (int i=0;i<n;i++)
 { 
     int x;
   cin>>x;
   v.push_back(x);
     
 }
   sort(v.begin (),v.end());//sorted by stl function sort()
  
   cout<<v[k-1 ]<<" "; //kth smallest element 
   cout<<v[n-k ]<<endl; //kth largest element 
	return 0;
}
